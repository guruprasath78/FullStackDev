package com.project.fitra.FitraController;


import java.util.Optional;


import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RequestParam;
import org.springframework.web.bind.annotation.RestController;

import com.project.fitra.FitraModel.FoodDbEntity;
import com.project.fitra.FitraService.FoodDbService;

@RestController
public class FoodDbController {

	@Autowired
	FoodDbService obj;
	@GetMapping("/getfooddb")
	public Optional<FoodDbEntity> getfooddb(@RequestParam String name)
	{
		return obj.getData(name);
	}
	@GetMapping("/getfood")
	public Optional<FoodDbEntity> getfood(@RequestParam String name)
	{
		System.out.print(name);
		return obj.getData(name);
	}
}
